{
  "overrides": [
    {
      "files": ["**/*.{js,jsx,json}"],
      "options": {
        "printWidth": 100,
        "useTabs": false,
        "tabWidth": 2,
        "singleQuote": true,
        "jsxSingleQuote": true,
        "semi": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "bracketSameLine": false
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ]
}
